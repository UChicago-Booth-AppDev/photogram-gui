<nav>
      <ul>
        <li><a href="/users">Users</a></li>
        <li><a href="/photos">Photos</a></li>
      </ul>
    </nav>
<h1><%=@user.username%></h1>

<dl>
  <dt>ID</dt>
    <dd><%=@user.id%></dd>
  <dt>Edit user</dt>
    <dd>Username</dd>
</dl>

<h2>Own Photos (<%=@user.own_photos.count%>)</h2>

<table border="1">
    <tr>
      <th>Image</th>
      <th>Owner</th>
      <th>Caption</th>
      <th>Posted</th>
    </tr>

    <%@user.own_photos.each do |a_photo|%>
    <tr>
      <td>
      <img src="<%=a_photo.image%>">
      </td>
      <td><%=a_photo.poster.username%></td>
      <td><%=a_photo.caption%></td>
      <td><%=a_photo.created_at%></td>
      <td>
        <a href=<%="/photos/#{a_photo.id}"%>>Show details</a>
      </td>
    </tr>
    <%end%>
</table>
